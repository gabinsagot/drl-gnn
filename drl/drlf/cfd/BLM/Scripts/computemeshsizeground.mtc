{ BLMUpdate2Ground=
    { Type= ModeleArithmetique }
    { Dependance= 
        { Maillage= MaillagePrincipal }
		{ Champ= BLMLevelSetLayer1Ground } 
		{ Champ= BLMZeroGround } 
		{ Champ= BLMFtyGround } 
    }
    { DependanceModifiable= 
		{ Champ= BLMLevelSetLayer2Ground }  
		{ Champ= BLMAppartient2Ground }  
    }	
	{ Operation= BLMLevelSetLayer2Ground = BLMLevelSetLayer1Ground }	
	{ Operation= BLMLevelSetLayer2Ground += BLMFtyGround }	
	{ Operation= BLMAppartient2Ground = BLMLevelSetLayer2Ground }	
	{ Operation= BLMAppartient2Ground >=? BLMZeroGround }	
}
