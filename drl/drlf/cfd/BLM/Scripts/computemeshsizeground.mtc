// *****************************************************
{ BLMInverseLevelSetGround=
   { Type= ModeleConditionnel }
   { Dependance=
      { Maillage= MaillagePrincipal }
      { ChampCondition= BLMInverseLevelSetGround }
      { ModeleTrue= BLMProblemLevelsetGround }
      { ModeleFalse= BLMLevelsetOKGround }
   }
}
{ BLMProblemLevelsetGround=
    { Type= ModeleArithmetique }
    { Dependance= 
        { Maillage= MaillagePrincipal }
        { Champ= BLMMOneGround }
        { Champ= BLMOneGround }
    }
    { DependanceModifiable= 
        { Champ= BLMLevelSetLayer1Ground } 
        { Champ= BLMHighlightGround } 
        { Champ= BLMAppartient1Ground } 
    }
	{ Operation= BLMLevelSetLayer1Ground *= BLMMOneGround }
	{ Operation= BLMHighlightGround = BLMOneGround }
	{ Operation= BLMHighlightGround -= BLMAppartient1Ground }
	{ Operation= BLMAppartient1Ground = BLMHighlightGround }
	{ Operation= BLMHighlightGround = BLMLevelSetLayer1Ground }	
}
{ BLMLevelsetOKGround=
    { Type= ModeleArithmetique }
    { Dependance= 
        { Maillage= MaillagePrincipal }
        { Champ= BLMMOneGround }
        { Champ= BLMOneGround }
		{ Champ= BLMLevelSetLayer1Ground } 
    }
    { DependanceModifiable= 
      { Champ= BLMHighlightGround }  
    }	
	{ Operation= BLMHighlightGround = BLMLevelSetLayer1Ground }	
}
